<Application x:Class="TDDD49.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TDDD49"
             Startup="AppStartup">

             <!--Startup="AppStartup"-->
    <Application.Resources>
        <SolidColorBrush x:Key="PrimaryBrush" Color="#2F82AC"/>
        <SolidColorBrush x:Key="DarkPrimaryBrush" Color="#2A759A"/>
        <SolidColorBrush x:Key="SecondaryBrush" Color="#ffffff"/>
        <SolidColorBrush x:Key="DarkSecondaryBrush" Color="#cccccc"/>
        <SolidColorBrush x:Key="PrimaryBorderBrush" Color="#cccccc"/>
    </Application.Resources>
</Application>
